<script type="text/javascript">
  Collab.Data.Project = {};
  Collab.Data.Project.Urls = {};
  Collab.Data.Project.projectId = 129;
  Collab.Data.Project.projectUrl = "http://collabfinder.com/project/129/Radius-environment-made-social";
  Collab.Data.Project.Urls.projectUrl = "http://collabfinder.com/project/129/Radius-environment-made-social";
  Collab.Data.Project.Urls.projectEditUrl = "http://collabfinder.com/project/edit/129";
  Collab.Data.Project.Urls.projectDeleteUrl = "http://collabfinder.com/project/delete/129";
  Collab.Data.Project.Urls.updateCreateUrl = "http://collabfinder.com/project/129/updates/create";
  Collab.Data.Project.Urls.updateDeleteUrl = "http://collabfinder.com/project/129/updates/delete";
  Collab.Data.Project.Urls.questionCreateUrl = "http://collabfinder.com/project/129/questions/create";
  Collab.Data.Project.Urls.anonQuestionCreateUrl = "http://collabfinder.com/project/129/questions/create_a";
  Collab.Data.Project.Urls.questionDeleteUrl = "http://collabfinder.com/project/129/questions/delete";
  Collab.Data.Project.Urls.answerCreateUrl = "http://collabfinder.com/project/129/questions/create_answer";
  Collab.Data.Project.Urls.answerDeleteUrl = "http://collabfinder.com/project/129/questions/delete_answer";
  Collab.Data.Project.Urls.emailFollowUrl = "http://collabfinder.com/project/129/follow/email";

$( function() {
  Galleria.loadTheme("//collabfinder.com/js/libs/galleria/themes/classic/galleria.collab.js?v=1365807841");
  $( "#project-photo-galleria" ).galleria();
  $("#project_page").on( "click", ".photo_pager", Collab.Project.photoDotClick );

  // Join button
  var join_button_selector = "#button-project-member-actions";
  if ($(join_button_selector).length > 0) {
    new Collab.ToggleStateButton(join_button_selector);
  }

  var follow_button_selector = "#button-follow-project";
  // Follow button
      $(follow_button_selector).on("click", function(e) {
      $('#follow_project').removeClass('off'); 
      $('#project_bg').removeClass('transparent_off');
    });

    $("#follow_project_form").on( "click", Collab.Project.Follow.submit );

    $("#follow_project_form").validate( { 
      errorPlacement: function( error, element ) {
        error.insertBefore( element.parent() );
      },
      submitHandler: Collab.Project.Follow.emailFollow
    });
  
      Collab.Tracking.track( "mixpanel", "View Project", { is_member: false } );

  
  
  //questions
  $("#question_form").validate( { 
    errorPlacement: function( error, element ) {
      error.insertBefore( element.parent() );
    }
  });

      $("#question_submit").on( "click", Collab.Project.Questions.unverifiedQuestionSubmit );
  
  
 
  
} );
</script>

 

